{
  "name": "bygamelist-server",
  "version": "1.0.0",
  "description": "Bygamelist is a platform that helps users find the servers they are looking for by listing the PVP servers of the most popular games.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "mongodb": "^4.10.0",
    "mongoose": "^8.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^22.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.4",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "author": "bygamelist developers",
  "license": "ISC",
  "keywords": [
    "bygamelist"
  ]
}
